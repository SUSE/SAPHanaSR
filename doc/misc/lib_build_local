function init()
{
    pdf="${stdDc#DC-}_color_en.pdf"
    dir=$PWD
    dc="${1:-$stdDc}" 
    fmt="pdf"
    timeStamp="$(date +'%Y%m%d-%H%M')"
}

function build()
{
    touch build/"$pdf"
    daps2docker "$dir/$dc" "$fmt"
}

function fetch()
{
    mkdir -p build
    find /tmp/daps2docker* \
              -name $pdf \
              -newer build/"$pdf" \
              -exec mv {} build \;
}
