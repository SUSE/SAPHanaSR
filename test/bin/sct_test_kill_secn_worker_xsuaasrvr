set -u
rc=1
src=${BASH_SOURCE[0]}
full_path=$(readlink -f "$src")
dir_path=$(dirname "$full_path")
source /usr/lib/SAPHanaSR-tester/sct_test_lib
source .test_properties
currSecnWorker=$(sctl_get_curr_secn_worker "$node01")

echo "==== Kill xsuaaserver on $currSecnWorker ===="
# TODO signal 9 ?
sctl_kill_process "$currSecnWorker" "$sidadm" "/usr/sap/$SID/HDB$instNr/exe/hdbxsuaaserver" "11" "flags:f:"; rc="$?"
exit "$rc"
#
