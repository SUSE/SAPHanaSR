#!/bin/bash
#
# sct_test_kill_secn_nameserver - kill secondary master nameserver
src=${BASH_SOURCE[0]}
full_path=$(readlink -f "$src")
dir_path=$(dirname "$full_path")
source /usr/bin/sct_test_lib
source .test_properties
currSecondary=$(sctl_get_curr_secn "$node01")

echo "==== Kill nameserver on $currSecondary ===="

sctl_kill_process "$currSecondary" "${sidadm}" "hdbnameserver" "-11"
# sleep 60
