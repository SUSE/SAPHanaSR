#!/bin/bash
#
# sct_test_unfreeze_secn_site_nfs - unfreeze nfs on secondary site
#
# (c) 2025 SUSE LLC
# Author: F.Herschel, L.Pinne.
# GNU General Public License v2. No warranty.
# http://www.gnu.org/licenses/gpl.html
#
set -u
src=${BASH_SOURCE[0]}
full_path=$(readlink -f "$src")
dir_path=$(dirname "$full_path")
source /usr/bin/sct_test_lib
source .test_properties
currSecondary=$(get_curr_secn "$node01")
currSecnWorker=$(get_curr_secn_worker "$node01")

echo "==== Un-Freeze SAP HANA NFS ===="

run_iptables "$currSecondary" 'iptables -D OUTPUT -p tcp -m multiport --ports 2049 -j DROP &'
run_iptables "$currSecnWorker" 'iptables -D OUTPUT -p tcp -m multiport --ports 2049 -j DROP'
sleep 60
#
