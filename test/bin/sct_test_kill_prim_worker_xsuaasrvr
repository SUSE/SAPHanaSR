set -u
rc=1
src=${BASH_SOURCE[0]}
full_path=$(readlink -f "$src")
dir_path=$(dirname "$full_path")
source /usr/lib/SAPHanaSR-tester/sct_test_lib
source .test_properties
currPrimWorker=$(sctl_get_curr_prim_worker "$node01")

echo "==== Kill xsuaaserver on $currPrimWorker ===="
# TODO signal 9 ?
sctl_kill_process "$currPrimWorker" "$sidadm" "/hana/shared/$SID/xs/bin/xsuaaserver" "11"  "flags:f:"; rc="$?"
exit "$rc"
#
