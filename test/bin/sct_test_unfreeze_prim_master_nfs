set -u
src=${BASH_SOURCE[0]}
full_path=$(readlink -f "$src")
dir_path=$(dirname "$full_path")
source /usr/bin/sct_test_lib
source .test_properties
currPrimary=$(get_curr_prim "$node01")
currPrimWorker=$(get_curr_prim_worker "$node01")

echo "==== Un-Freeze SAP HANA NFS ===="

run_iptables "$currPrimary" 'iptables -D OUTPUT -p tcp -m multiport --ports 2049 -j DROP'
sleep 60
#
